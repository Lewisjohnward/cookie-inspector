{
  "manifest_version": 2,
  "version": "1.0",
  "name": "Cookie Inspector",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icons/star.png"
  },
  "permissions": [
    "cookies"
  ]
}
